<?xml version="1.0"?>
<card xmlns="http://sourceforge.net/projects/firemox" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://sourceforge.net/projects/firemox ../../validator.xsd" name="Bog Wraith">
<!-- Swampwalk (This creature is unblockable as long as defending player controls a Swamp.) -->
	<rules-author-comment>By brius.</rules-author-comment>
	<init>
		<registers>
			<register index="black" value="1"/>
			<register index="colorless" value="3"/>
			<register index="power" value="3"/>
			<register index="toughness" value="3"/>
		</registers>
		<colors>black</colors>
		<idcards>creature</idcards>
		<properties>wraith swampwalk</properties>
	</init>

	<abilities>
		<ability ref="cast-spell"/>
	</abilities>
</card>
